package com.vicky.AnnotationSetter.Config;

import org.springframework.context.annotation.Bean;
import org.springframework.context.annotation.Configuration;

import com.vicky.DIDependentObject.POJO.Brand;
import com.vicky.DIDependentObject.POJO.Mobile;
import com.vicky.DIDependentObject.POJO.Ram;

@Configuration
public class AppConfig {
	
	@Bean
	public Address addressBean()
	{
		return new Address();
	}
	
	@Bean
	public SJT sjtBean()
	{
		return new SJT();
	}
	

}
